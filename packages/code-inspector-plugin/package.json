{
  "name": "@neurora/code-inspector-plugin",
  "version": "1.3.4",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./types/index.d.ts",
  "repository": "git@github.com:zh-lx/code-inspector.git",
  "author": "zh-lx <18366276315@163.com>",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf ./dist && rimraf ./types",
    "build": "pnpm clear && tsc && vite build",
    "pub": "pnpm publish",
    "pub:beta": "pnpm publish --tag beta"
  },
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./dist/*": {
      "import": {
        "default": "./dist/*"
      },
      "require": {
        "default": "./dist/*"
      }
    }
  },
  "homepage": "https://inspector.fe-dev.cn/en",
  "description": "Click the dom on the page, it will open your IDE and position the cursor to the source code location of the dom.",
  "keywords": [
    "webpack",
    "vite",
    "plugin",
    "vue",
    "vscode",
    "inspector",
    "inspect"
  ],
  "bugs": {
    "url": "https://github.com/zh-lx/code-inspector/issues"
  },
  "dependencies": {
    "chalk": "4.1.1",
    "@neurora/code-inspector-core": "^1.3.1",
    "@neurora/code-inspector-vite": "^1.3.1",
    "@code-inspector/webpack": "^1.2.7",
    "@code-inspector/esbuild": "^1.2.7",
    "@code-inspector/turbopack": "^1.2.7",
    "@code-inspector/mako": "^1.2.7"
  },
  "devDependencies": {
    "@types/node": "^16.0.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}